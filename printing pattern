// Java program that accepts
// array input from the user.
import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int c;
        int arr[][]=new int[n][];
        for(int i=0;i<=n;i++){
            c=s.nextInt();
            arr[i]=new int[c];
        }
        for(int i=0;i<=n;i++){
            for(int j:arr[i]){
                System.out.println(j+" ");
            }
            System.out.println();
        }
    }
}
